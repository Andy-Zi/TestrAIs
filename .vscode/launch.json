  "version": "0.2.0",
  "configurations": [

    {
      "name": "Python: Main",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/main.py",
      "console": "integratedTerminal",
      "justMyCode": true,
      "env": {
        "PYTHONDONTWRITEBYTECODE": "1", 
      }
    },
    {
      "name": "Python: Agent",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/Agent/tetrisAgent.py",
      "console": "integratedTerminal",
      "justMyCode": true,
      "env": {
        "PYTHONPATH": "${workspaceFolder}",
        "PYTHONDONTWRITEBYTECODE": "1", 
      }
    }
  ]
}